{
  "name": "nodejs-quickstart",
  "version": "0.1.0",
  "license": "MPL-2.0",
  "description": "Node.js Quickstart/Demo app",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js --watch src/server --watch src/shared",
    "build": "webpack --progress -p",
    "build-and-watch": "webpack --progress --watch",
    "webpack-dev-iframe": "webpack-dev-server --port 8081 --content-base http://localhost:8080",
    "webpack-dev-inline": "webpack-dev-server --port 8081 --inline",
    "all": "concurrently --kill-others \"yarn run webpack-dev-inline\" \"yarn run start\"",
    "deploy": "lt --port 8080 -s nodejsapp -o"
  },
  "author": "Dalimil Hajek",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "connect-mongo": "^1.2.0",
    "cookies": "^0.6.2",
    "crypto": "^0.0.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "mongoose": "^4.4.19",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-alpha7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "request": "^2.72.0",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.1.0",
    "localtunnel": "^1.8.2",
    "nodemon": "^1.9.2",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
